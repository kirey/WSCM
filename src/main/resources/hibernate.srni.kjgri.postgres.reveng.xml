<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >


<hibernate-reverse-engineering>

	<schema-selection match-schema="sch_kjgri" />


	<type-mapping>

		<sql-type hibernate-type="float" jdbc-type="FLOAT" />
		<sql-type hibernate-type="float" jdbc-type="REAL" />
		<sql-type hibernate-type="double" jdbc-type="DOUBLE" />
		<sql-type hibernate-type="String" jdbc-type="VARCHAR" />
		<sql-type hibernate-type="String" jdbc-type="CHAR" />
		<sql-type hibernate-type="Integer" jdbc-type="INTEGER" />
		<sql-type hibernate-type="Short" jdbc-type="SMALLINT" />
		<sql-type hibernate-type="Long" jdbc-type="BIGINT" />
		<sql-type hibernate-type="java.util.Date" jdbc-type="DATE"
			length="7">
		</sql-type>
		<sql-type hibernate-type="java.util.Date" jdbc-type="TIMESTAMP" />
		<sql-type hibernate-type="byte[]" jdbc-type="BLOB" />
		<sql-type hibernate-type="byte[]" jdbc-type="VARBINARY" />
		<!-- <sql-type hibernate-type="String" jdbc-type="TEXT" /> -->
		<!-- <sql-type hibernate-type="byte[]" jdbc-type="BYTEA" /> -->

		<sql-type hibernate-type="String" jdbc-type="CLOB" not-null="true"/>
	

		<sql-type hibernate-type="Integer" jdbc-type="NUMERIC"
			precision="10" scale="0"/>
		
		<sql-type hibernate-type="Long" jdbc-type="NUMERIC"
			precision="19" scale="0"/>
		
		<sql-type hibernate-type="Long" jdbc-type="NUMERIC" scale="0"/>
		
		<sql-type hibernate-type="BigDecimal" jdbc-type="NUMERIC"/>
			<sql-type hibernate-type="Boolean" jdbc-type="BIT"/>
			<!-- <sql-type hibernate-type="Boolean" jdbc-type="BOOLEAN"/> -->
		<sql-type hibernate-type="java.util.Date" jdbc-type="OTHER" />
	</type-mapping>

</hibernate-reverse-engineering>