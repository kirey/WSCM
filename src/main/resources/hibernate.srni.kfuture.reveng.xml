<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >


<hibernate-reverse-engineering>
	<schema-selection match-schema="KFUTURE" />

	<type-mapping>

		<sql-type jdbc-type="FLOAT" hibernate-type="double"></sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"></sql-type>
		<sql-type jdbc-type="BLOB" hibernate-type="byte[]"></sql-type>
		<sql-type jdbc-type="OTHER" hibernate-type="java.util.Date" />
		<sql-type jdbc-type="DATE" hibernate-type="java.util.Date"
			length="7" not-null="true">
		</sql-type>
		<sql-type jdbc-type="CLOB" hibernate-type="string" not-null="true">
		</sql-type>
		<sql-type jdbc-type="NUMERIC" hibernate-type="Integer"
			precision="10" not-null="true">
		</sql-type>
		<sql-type jdbc-type="NUMERIC" hibernate-type="Long" not-null="false">
		</sql-type>
	</type-mapping>
</hibernate-reverse-engineering>